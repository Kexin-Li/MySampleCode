<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping
PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="com.entity.Order" table="h_order">
		
		<id name="oderid" column="ORDERID" type="java.lang.Integer">
			<generator class="identity" />
		</id>
		
		<property name="userid" column="USERID" type="java.lang.Integer" />
		<property name="roomid" column="ROOMID" type="java.lang.Integer" />
	</class>
</hibernate-mapping>